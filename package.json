{
  "name": "console-generator",
  "displayName": "console-generator",
  "description": "一个console快捷生成插件，提供快速生成console语句，让你打印快人一步",
  "version": "0.0.2",
  "publisher": "Jianganfu",
  "engines": {
    "vscode": "^1.72.0"
  },
  "keywords": [
    "refactor",
    "javascript",
    "typescript",
    "console",
    "tools"
  ],
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:console-generator.generate-console",
    "onCommand:console-generator.delete-file-console"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "console-generator.generate-console",
        "title": "生成console语句"
      },
      {
        "command": "console-generator.delete-file-console",
        "title": "删除文件中的console语句"
      }
    ],
    "keybindings": [
      {
        "command": "console-generator.generate-console",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l"
      },
      {
        "command": "console-generator.delete-file-console",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "console生成器",
      "properties": {
        "consoleGenerator.enable": {
          "type": "boolean",
          "default": true,
          "description": "是否启用插件"
        },
        "consoleGenerator.semi": {
          "type": "boolean",
          "default": true,
          "description": "是否以分号结尾"
        },
        "consoleGenerator.customPrefix.template": {
          "type": "string",
          "description": "生成console语句的模板,#i 标识符;#f 展示文件名;#n 展示行号;"
        },
        "consoleGenerator.customPrefix.style": {
          "type": "string",
          "description": "自定义生成console语句的样式"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "vscode:publish": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:unit": "vitest -r src"
  },
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@types/babel__traverse": "^7.18.2",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "@babel/parser": "^7.19.6",
    "@babel/traverse": "^7.19.6",
    "@vue/compiler-sfc": "^3.2.41"
  }
}
